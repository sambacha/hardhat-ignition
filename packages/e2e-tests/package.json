{
  "name": "e2e-tests",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.18",
    "@types/fs-extra": "^9.0.11",
    "chai": "^4.3.4",
    "fs-extra": "^10.0.0",
    "mocha-sinon": "^2.1.2",
    "nyc": "^15.1.0",
    "shelljs": "^0.8.4",
    "ts-node": "^9.1.1"
  },
  "scripts": {
    "lint:fix": "node ../../node_modules/prettier/bin-prettier.js --write \"./**/*.{js,ts}\" && yarn lint --fix",
    "lint": "node -r ts-node/register/transpile-only ../../node_modules/tslint/bin/tslint --config tslint.json --project ./tsconfig.json",
    "clean": "node ../../node_modules/rimraf/bin.js dist",
    "compile": "node ../../node_modules/typescript/bin/tsc --noEmit",
    "build": "node ../../node_modules/typescript/bin/tsc --build .",
    "test": "node ../../node_modules/mocha/bin/mocha --config ./.mocharc.yaml \"test/*/**/*.test.ts\"",
    "test:deploy": "node ../../node_modules/mocha/bin/mocha --config ./.mocharc.yaml \"test/actions/deploy.test.ts\"",
    "test:diff": "node ../../node_modules/mocha/bin/mocha --config ./.mocharc.yaml \"test/actions/diff.test.ts\""
  },
  "dependencies": {
    "cli-ux": "^5.5.1",
    "ethers": "^5.1.4",
    "mocha": "^8.4.0",
    "typescript": "^4.2.4"
  }
}
