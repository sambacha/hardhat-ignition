    "@types/chai": "^4",
    "@types/cli-progress": "^3.8.0",
    "@types/cls-hooked": "^4.3.1",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "ethereum-waffle": "^3.2.0",
    "globby": "^10",
    "hardhat": "^2.0.3",
    "local-package-publisher": "^1.0.4",
    "mocha": "^5",
    "nyc": "^14",
    "tslint": "^6.1.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/nomiclabs/hardhat-ignition",
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "hardhat-ignition",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nomiclabs/hardhat-ignition.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "pack": "oclif-dev pack",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "type": "commonjs",
  "directories": {
    "doc": "docs",
    "example": "example",
    "lib": "lib",
    "test": "test"
  }
