    "elegant-spinner": "^2.0.0",
    "ethers": "^5.0.22",
    "figures": "^3.2.0",
    "fs-extra": "^9.0.1",
    "indent-string": "^4.0.0",
    "inquirer": "^8.0.0",
    "listr": "^0.14.3",
    "log-symbols": "^4.0.0",
    "log-update": "^4.0.0",
    "path": "^0.12.7",
    "rxjs": "^6.6.3",
    "solc": "^0.7.5",
    "strip-ansi": "^6.0.0",
    "ts-node": "^9.1.1",
    "tslib": "^1",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@google-cloud/storage": "^5.6.0",
